<ETHERNETKRL>
  <CONFIGURATION>
    <EXTERNAL>
      <TYPE>Client</TYPE>
    </EXTERNAL>
    <INTERNAL>
      <ENVIRONMENT>Submit</ENVIRONMENT>
      <BUFFERING Limit="512" />
      <ALIVE Set_Flag="105" Ping="120" />
      <IP>172.31.1.147</IP>
      <PORT>54600</PORT>
      <PROTOCOL>TCP</PROTOCOL>
      <MESSAGES Logging="warning" Display="disabled" />
    </INTERNAL>
  </CONFIGURATION>
  <RECEIVE>
